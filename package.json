{
  "name": "json-literal-typer",
  "version": "0.0.5",
  "description": "generates literal typescript iterfaces from JSON",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean:test": "rimraf src/__tests__/out/* coverage/*",
    "clean:build": "rimraf lib/*",
    "pretest": "npm run clean:test",
    "test": "jest",
    "pretest:coverage": "npm run clean:test",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "prebuild": "npm run clean:build",
    "build": "tsc",
    "lint": "eslint \"src/**/*.{js,ts,jsx,tsx}\"",
    "format": "prettier --write \"src/**/*.{ja,ts,jsx,tsx,json,html,css,scss,yml,yaml}\"",
    "format:check": "prettier --check \"src/**/*.{ja,ts,jsx,tsx,json,html,css,scss,yml,yaml}\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint"
  },
  "keywords": [
    "json",
    "typescript",
    "literal"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pabra/json-literal-typer.git"
  },
  "contributors": [
    "Michael Scott-Nelson <mscottnelson@gmail.com> (https://github.com/mscottnelson)"
  ],
  "bugs": {
    "url": "https://github.com/pabra/json-literal-typer/issues"
  },
  "homepage": "https://github.com/pabra/json-literal-typer#readme",
  "author": "Patrick Braune <pabra@netzfruech.de>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.4.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
    "typescript": "^3.6.4"
  },
  "peerDependencies": {},
  "dependencies": {}
}
